@model List<Department>
@{
    ViewData["Title"] = "Index";
}
<a href="/Department/New">New Department</a>

<table class="table table-bordered table-hover">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>ManagerName</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.ManagerName</td>
        </tr>
    }
    
</table>

